#include <iostream>
using namespace std;

void print(int arr[], int size) {
    for (int i = 0; i < size; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
}

void reverse(int arr[], int size) {
    for (int i = 0; i < size / 2; i++) {
        int temp = arr[i];
        arr[i] = arr[size - 1 - i];
        arr[size - 1 - i] = temp;
    }
}

int main() {
    const int Size = 10;
    int arr[Size];

    for (int i = 0; i < Size; i++) {
        arr[i] = i + 1;
    }

    print(arr, Size);
    reverse(arr, Size);
    print(arr, Size);

    return 0;
}
