#include <iostream>
using namespace std;

int main() {
    srand(time(0));
    const unsigned N = 15;
    int arr[N];

    for (int i = 0; i < N; i++) {
        arr[i] = rand() % 200;
        cout << arr[i] << ' ';
    }
    cout << endl;

    int sum = 0;
    bool safepul = true;

    for (int i = 0; i < N; i++) {
        sum += arr[i];
        if (arr[i] < 120 || arr[i] > 160) {
            safepul = false;
        }
    }

    double average = (double)sum / N;
    cout << "Ser puls " << average << endl;

    if (safepul) {
        cout << "Puls 120-160" << endl;
    }
    else {
        cout << "NE" << endl;
    }

}



